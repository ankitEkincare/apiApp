<br>
<div class="row row-cols-1 row-cols-md-4">
  <% @output.each do |user| %>
    <div class="card" style="width: 18rem;">
      <img src=<%= user['avatar'] %> class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title"><%= user['name']%></h5>
        <p class="card-text"><%= user['description']%></p>
      </div>
    </div>
  <% end %>
</div>